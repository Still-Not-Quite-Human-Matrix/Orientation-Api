<!DOCTYPE html>
<html>
<head>
        <link rel="icon"
              type="image/png"
              href="https://avatars1.githubusercontent.com/u/37025111?s=400&amp;u=33d2b88200e3b4de784fa905897cc5acf4c60fa2&amp;v=4">
         <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>@ViewBag.Title Bangazon By: SNQHM </title>
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body ng-app="StillNotQuiteHuman">
        <div class="navbar navbar-inverse navbar-fixed-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a ng-href="#!" class="navbar-brand">Still Not Quite Human</a>
                    <a ng-href="#!/computers" class="navbar-brand">Computers</a>
                    <a ng-href="#!/employees" class="navbar-brand">Employees</a>
                </div>
                <div class="navbar-collapse collapse">
                    <ul class="nav navbar-nav"></ul>
                </div>
            </div>
        </div>

        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - Bangazon. By: SNQHM</p>
            </footer>
        </div>

        @Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")
        @Scripts.Render("~/bundles/angular")
        @RenderSection("scripts", required: false)
    </body>
</html>